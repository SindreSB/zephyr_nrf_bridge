
{
    "env": {
        "toolchain_root": "C:/gnu_arm_embedded",
        "arch_includes": [
            "${ZEPHYR_BASE}/arch/arm/include/**",
            "${ZEPHYR_BASE}/arch/arm/core/cortex_m/**"
        ],
        "hal_includes" : [
            "${ZEPHYR_BASE}/ext/hal/st/stm32cube/stm32l4xx/soc/**",
            "${ZEPHYR_BASE}/ext/hal/st/stm32cube/stm32l4xx/drivers/include/**"
        ],
        "soc_includes" : [
            "${ZEPHYR_BASE}/soc/arm/st_stm32/common",
            "${ZEPHYR_BASE}/soc/arm/st_stm32/stm32l4/include"
        ],
        "zephyr_includes": [
            "${ZEPHYR_BASE}/kernel/include/**",
            "${ZEPHYR_BASE}/include/**",
            "${ZEPHYR_BASE}/lib/libc/minimal/include/**",
            "${ZEPHYR_BASE}/drivers/**",
            "${ZEPHYR_BASE}/ext/hal/cmsis/Include/**"
        ]
    },
    "configurations": [
        {
            "name": "Win",
            "intelliSenseMode": "clang-x64",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "compilerPath": "${toolchain_root}/bin/arm-none-eabi-gcc.exe",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "includePath": [
                "${arch_includes}",
                "${soc_includes}",
                "${hal_includes}",
                "${zephyr_includes}",
                "${workspaceFolder}/build/zephyr/include/**"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:ZEPHYR_BASE}"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}